# If you want to use XFCE config tools...
#
#xfce-mcs-manager &

xwallpaper --daemon --zoom /usr/share/pixmaps/wallpaper
lxpolkit &
picom -bc
setxkbmap -option ctrl:nocaps
tint2 -c $HOME/.config/tint2/tint2panel &
urxvtd -q -o -f
thunar --daemon &
xset s 900
xss-lock lock-manager &
